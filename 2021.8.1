#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
#include<string.h>

int main()
{
	int i = 0;
	char   passworld[20] = { 0 };
	for (i=0;i<3;i++)
	{
		scanf("请输入密码：%s\n", passworld);
		if (strcmp(passworld, "123456") == 0)
		{
			printf("登录成功\n");
			break;
		}
		else
		{
			printf("密码错误\n");
			
		}
	}
	if (i == 3)
	{
		printf("密码错误三次，退出程序\n");
	}

	return 0;
}
